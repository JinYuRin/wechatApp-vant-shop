@font-face {
  font-family: 'iconfont';
  /* project id 1556606 */
  src: url('//at.alicdn.com/t/font_1556606_k0xshxy52pn.eot');
  src: url('//at.alicdn.com/t/font_1556606_k0xshxy52pn.eot?#iefix') format('embedded-opentype'),
    url('//at.alicdn.com/t/font_1556606_k0xshxy52pn.woff2') format('woff2'),
    url('//at.alicdn.com/t/font_1556606_k0xshxy52pn.woff') format('woff'),
    url('//at.alicdn.com/t/font_1556606_k0xshxy52pn.ttf') format('truetype'),
    url('//at.alicdn.com/t/font_1556606_k0xshxy52pn.svg#iconfont') format('svg');
}

/*引入font-family要强调优先级*/
.iconfont {
  font-family: iconfont !important;
  font-size: 80rpx !important;
}

/*:before是伪类啊*/
.l-icon-home:before {
  content: "\e611";
